{
  "name": "overlay-live-comment-viewer",
  "version": "1.1.0",
  "description": "The abbreviation is OLCV.  This app overlays live comments on your desktop.",
  "main": "build/main.js",
  "scripts": {
    "develop": "run-s clean:output build:dev serve",
    "production": "run-s clean:output clean:cache build:pro package:winx86",
    "jest": "jest",
    "e2e": "echo todo",
    "lint": "eslint src",
    "serve": "electron .",
    "asar": "asar extract dist/win-unpacked/resources/app.asar dist/win-unpacked/resources/app",
    "clean:output": "rimraf build dist public",
    "clean:cache": "rimraf node_modules/.cache",
    "build:dev": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:pro": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package:winx64": "electron-builder --win --x64 --config ./electron-builder-x64.yml",
    "package:winx86": "electron-builder --win --ia32",
    "package:macx64": "electron-builder --mac --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LenTakayama/Overlay-Live-Comment-Viewer.git"
  },
  "author": "LenTakayama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer/issues"
  },
  "homepage": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/preset-env": "^7.17.10",
    "@types/jest": "^27.5.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "asar": "^3.1.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "semver": "^7.3.7",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "thread-loader": "^3.0.4",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-remove-empty-scripts": "^0.8.0"
  }
