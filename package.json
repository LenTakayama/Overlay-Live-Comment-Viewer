{
  "name": "overlay-live-comment-viewer",
  "version": "1.0.2",
  "description": "The abbreviation is OLCV.  This app overlays live comments on your desktop.",
  "main": "build/main.js",
  "scripts": {
    "develop": "run-s clean:output build:dev serve",
    "production": "run-s clean:output clean:cache build:pro package:winx86",
    "jest": "jest",
    "e2e": "echo todo",
    "lint": "eslint src",
    "scan": "run-s build:pro electronegativity",
    "serve": "electron .",
    "asar": "asar extract dist/win-unpacked/resources/app.asar dist/win-unpacked/resources/app",
    "clean:output": "rimraf build dist public",
    "clean:cache": "rimraf node_modules/.cache",
    "build:dev": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:pro": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package:winx64": "electron-builder --win --x64 --config ./electron-builder-x64.yml",
    "package:winx86": "electron-builder --win --ia32",
    "package:macx64": "electron-builder --mac --x64",
    "electronegativity": "electronegativity -i ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LenTakayama/Overlay-Live-Comment-Viewer.git"
  },
  "author": "LenTakayama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer/issues"
  },
  "homepage": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer#readme",
  "engines": {
    "node": "14.16.0",
    "npm": "7.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/preset-env": "^7.13.15",
    "@types/jest": "^26.0.22",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "asar": "^3.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-log": "^4.3.4",
    "electron-store": "^7.0.3",
    "electron-updater": "^4.3.8",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "fibers": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "postcss-sorting": "^6.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "spectron": "^14.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.1",
    "thread-loader": "^3.0.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-fix-style-only-entries": "^0.6.1"
  }
}
