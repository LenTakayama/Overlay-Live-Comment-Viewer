{
  "name": "overlay-live-comment-viewer",
  "version": "1.0.3",
  "description": "The abbreviation is OLCV.  This app overlays live comments on your desktop.",
  "main": "build/main.js",
  "scripts": {
    "develop": "run-s clean:output build:dev serve",
    "production": "run-s clean:output clean:cache build:pro package:winx86",
    "jest": "jest",
    "e2e": "echo todo",
    "lint": "eslint src",
    "scan": "run-s build:pro electronegativity",
    "serve": "electron .",
    "asar": "asar extract dist/win-unpacked/resources/app.asar dist/win-unpacked/resources/app",
    "clean:output": "rimraf build dist public",
    "clean:cache": "rimraf node_modules/.cache",
    "build:dev": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:pro": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package:winx64": "electron-builder --win --x64 --config ./electron-builder-x64.yml",
    "package:winx86": "electron-builder --win --ia32",
    "package:macx64": "electron-builder --mac --x64",
    "electronegativity": "electronegativity -i ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LenTakayama/Overlay-Live-Comment-Viewer.git"
  },
  "author": "LenTakayama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer/issues"
  },
  "homepage": "https://github.com/LenTakayama/Overlay-Live-Comment-Viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "asar": "^3.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-log": "^4.3.5",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "fibers": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-loader": "^6.1.0",
    "postcss-sorting": "^6.0.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "spectron": "^15.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.3",
    "thread-loader": "^3.0.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-fix-style-only-entries": "^0.6.1"
  }
}
